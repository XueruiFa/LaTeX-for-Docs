{
  "manifest_version": 1

  "name": "LaTeX for Docs",
  "description": "A tool to be able to use LaTeX symbols in Google Docs",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [background.js]
  },
  //Persistence should be true
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}